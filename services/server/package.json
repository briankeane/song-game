{
  "name": "playola-server",
  "version": "1.0.0",
  "description": "Creates and maintains radio stations based on the users's Spotify Account",
  "main": "index.js",
  "scripts": {
    "sequelize": "sequelize",
    "migrate": "sequelize db:migrate",
    "migrate:test": "env-cmd -f .env-test sequelize db:migrate",
    "migration:create": "sequelize migration:generate",
    "start": "env-cmd -f .env nodemon -L server.js",
    "test-local": "env-cmd .env-test mocha --compilers js:babel-register --require babel-polyfill",
    "test": "env-cmd -f .env-test mocha \"./{,!(node_modules)/**}/*.test.js\" --recursive --exit",
    "lint": "eslint ."
  },
  "author": "Playola Radio Inc",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "errorhandler": "^1.5.1",
    "express": "^4.16.3",
    "express-bearer-token": "^2.1.1",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.1.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "faker": "^4.1.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "sinon": "^9.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "10.16.3"
  }
}
